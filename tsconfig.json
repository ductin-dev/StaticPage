{
    "extends": "./tsconfig.paths.json",
    "compilerOptions": {
        "jsx": "preserve",
        "target": "ES6",
        "module": "ESNext",
        "moduleResolution": "node",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "esModuleInterop": true,
        "strictPropertyInitialization": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": false,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "incremental": true
    },
    "ts-node": {
        "require": [
            "tsconfig-paths/register"
        ]
    },
    "typeRoots": [
        ".src/Application/definition/type.d.ts",
        "./node_modules/@types",
        "./node_modules/@types/node"
    ],
    "include": [
        ".src/Application/definition/type.d.ts",
        ".src/Application/definition/next-env.d.ts",
        ".src/Application/definition/react-app-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        "**/*.js",
        "**/*.jsx",
        "**/*.wasm",
        "**/*.mjs",
        "**/*.json"
, "src/Middleware/layout/LayoutMiddlewarecopy"    ],
    "exclude": [
        "node_modules",
        "lib"
    ]
}